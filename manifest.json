{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "*://youtube.com/*",
                "*://www.youtube.com/*",
                "*://m.youtube.com/*"
            ],
            "exclude_matches": [
                "*://*.music.youtube.com/*"
            ],
            "all_frames": true,
            "js": [
                "contentscript.js"
            ],
            "run_at": "document_end"
        }
    ],
    "action": {
        "default_icon": {
            "16": "/images/get_started16.png",
            "32": "/images/get_started32.png",
            "48": "/images/get_started48.png",
            "128": "/images/get_started128.png"
        }
    },
    "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
    }
}